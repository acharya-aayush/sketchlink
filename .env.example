# ================================
# SketchLink Environment Variables
# ================================
# Copy this file to .env and fill in your values

# Server port for local development
PORT=3001

# Production server URL (your Render backend URL)
# REQUIRED for production - set in Vercel/Netlify dashboard
# Example: https://your-app-name.onrender.com
VITE_SERVER_URL=

# Node environment
NODE_ENV=development
